        <rdfs:subClassOf rdf:resource="https://gitlab.atb-bremen.de/atb/pss-ontology/PSS-ontology-2#Stakeholder"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://gitlab.atb-bremen.de/atb/pss-ontology/PSS-ontology-2#owns"/>
